{
    "version": 2,
    "builds": [
        {
            "src": "python_vercel_task/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9.7"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/python_vercel_task/wsgi.py"
        }
    ]
}